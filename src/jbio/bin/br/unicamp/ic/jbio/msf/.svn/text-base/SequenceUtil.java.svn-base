package br.unicamp.ic.jbio.msf;

public class SequenceUtil {
	
	private static final String GAP_CHARACTERS = "[._-]";
    private static final String NORMALIZED_GAP = "-";


    public static String NormalizeGaps(String sequence)
    {
        String returnValue = null;
        
        returnValue = sequence.replaceAll(GAP_CHARACTERS, NORMALIZED_GAP);

        return returnValue;
    }

}
