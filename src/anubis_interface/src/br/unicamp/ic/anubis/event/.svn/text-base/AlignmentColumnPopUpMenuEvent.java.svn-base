package br.unicamp.ic.anubis.event;

import java.awt.Component;
import java.awt.Point;

public class AlignmentColumnPopUpMenuEvent extends AlignmentShowPopUpMenuEvent {
	
	private int selectedColumn;

	public AlignmentColumnPopUpMenuEvent(int alignmentId, int selectedColumn, Point point, Component container) {
		super(alignmentId, point, container);
		this.selectedColumn = selectedColumn;
	}

	public int getSelectedColumn() {
		return selectedColumn;
	}


}
